{
  "name": "cy-language",
  "displayName": "Cyper Language Support",
  "description": "Syntax highlighting, snippets, and language configuration for Cyper (.cy).",
  "version": "0.1.1",
  "publisher": "suryasekhar06jemsbond-lab",
  "license": "UNLICENSED",
  "icon": "icons/cy-logo.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "cy",
    "cyper",
    "cypher",
    "language",
    "compiler",
    "runtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/suryasekhar06jemsbond-lab/cyber.git",
    "directory": "editor/vscode/cy-language"
  },
  "contributes": {
    "languages": [
      {
        "id": "cy",
        "aliases": [
          "Cyper",
          "cyper",
          "Cy",
          "cy"
        ],
        "extensions": [
          ".cy"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/cy-file-light.png",
          "dark": "./icons/cy-file-dark.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "cy",
        "scopeName": "source.cy",
        "path": "./syntaxes/cy.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "cy",
        "path": "./snippets/cy.json"
      }
    ],
    "iconThemes": [
      {
        "id": "cy-seti-icons",
        "label": "Cyper Seti Icons",
        "path": "./icons/cy-seti-icon-theme.json"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.1.0"
  }
}
